{
  "title": "Personalized Tourist Planner Output",
  "description": "Generated itinerary with cost breakdown and recommendations",
  "type": "object",
  "actorOutputSchemaVersion": 1,
  "properties": {
    "success": {
      "type": "boolean",
      "description": "Whether the itinerary generation was successful",
      "template": "boolean"
    },
    "startLocation": {
      "type": "object",
      "description": "Geocoded start location details",
      "template": "object",
      "properties": {
        "name": {
          "type": "string",
          "template": "string"
        },
        "latitude": {
          "type": "number",
          "template": "number"
        },
        "longitude": {
          "type": "number",
          "template": "number"
        }
      }
    },
    "totalCost": {
      "type": "number",
      "description": "Total estimated cost in INR",
      "template": "number"
    },
    "costBreakdown": {
      "type": "object",
      "description": "Detailed cost breakdown",
      "template": "object",
      "properties": {
        "fuel": {
          "type": "number",
          "template": "number"
        },
        "stay": {
          "type": "number",
          "template": "number"
        },
        "food": {
          "type": "number",
          "template": "number"
        },
        "attractions": {
          "type": "number",
          "template": "number"
        },
        "misc": {
          "type": "number",
          "template": "number"
        }
      }
    },
    "costEstimateAccuracy": {
      "type": "string",
      "description": "Accuracy range of cost estimate",
      "template": "string"
    },
    "itinerary": {
      "type": "array",
      "description": "Day-by-day itinerary",
      "template": "array",
      "items": {
        "type": "object",
        "template": "object",
        "properties": {
          "day": {
            "type": "integer",
            "template": "number"
          },
          "places": {
            "type": "array",
            "template": "array",
            "items": {
              "type": "string",
              "template": "string"
            }
          },
          "distance": {
            "type": "number",
            "description": "Distance for the day in km",
            "template": "number"
          },
          "cost": {
            "type": "number",
            "description": "Estimated cost for the day",
            "template": "number"
          },
          "activities": {
            "type": "string",
            "description": "List of activities and notes",
            "template": "string"
          },
          "accommodation": {
            "type": "string",
            "description": "Recommended accommodation type",
            "template": "string"
          },
          "meals": {
            "type": "array",
            "template": "array",
            "items": {
              "type": "string",
              "template": "string"
            },
            "description": "Meal recommendations"
          },
          "weatherForecast": {
            "type": "string",
            "description": "Weather forecast if available",
            "template": "string"
          }
        }
      }
    },
    "attractions": {
      "type": "array",
      "description": "Detailed information about attractions",
      "template": "array",
      "items": {
        "type": "object",
        "template": "object",
        "properties": {
          "name": {
            "type": "string",
            "template": "string"
          },
          "type": {
            "type": "string",
            "template": "string"
          },
          "location": {
            "type": "string",
            "template": "string"
          },
          "distanceFromStart": {
            "type": "number",
            "template": "number"
          },
          "description": {
            "type": "string",
            "template": "string"
          },
          "entryFee": {
            "type": "number",
            "template": "number"
          },
          "openingHours": {
            "type": "string",
            "template": "string"
          },
          "coordinates": {
            "type": "object",
            "template": "object",
            "properties": {
              "latitude": {
                "type": "number",
                "template": "number"
              },
              "longitude": {
                "type": "number",
                "template": "number"
              }
            }
          }
        }
      }
    },
    "transportation": {
      "type": "object",
      "description": "Transportation details",
      "template": "object",
      "properties": {
        "vehicleType": {
          "type": "string",
          "template": "string"
        },
        "totalDistanceKm": {
          "type": "number",
          "template": "number"
        },
        "fuelNeeded": {
          "type": "number",
          "description": "Estimated fuel needed in liters",
          "template": "number"
        },
        "fuelCostEstimate": {
          "type": "number",
          "template": "number"
        },
        "mileage": {
          "type": "number",
          "description": "Assumed km per liter",
          "template": "number"
        },
        "chargingStops": {
          "type": "array",
          "template": "array",
          "items": {
            "type": "string",
            "template": "string"
          },
          "description": "EV charging stations if applicable"
        }
      }
    },
    "accommodations": {
      "type": "array",
      "description": "Recommended accommodations",
      "template": "array",
      "items": {
        "type": "object",
        "template": "object",
        "properties": {
          "location": {
            "type": "string",
            "template": "string"
          },
          "type": {
            "type": "string",
            "template": "string"
          },
          "pricePerNight": {
            "type": "number",
            "template": "number"
          },
          "rating": {
            "type": "string",
            "template": "string"
          },
          "amenities": {
            "type": "array",
            "template": "array",
            "items": {
              "type": "string",
              "template": "string"
            }
          }
        }
      }
    },
    "risks": {
      "type": "array",
      "description": "Potential risks and considerations",
      "template": "array",
      "items": {
        "type": "string",
        "template": "string"
      }
    },
    "recommendations": {
      "type": "array",
      "description": "Additional recommendations",
      "template": "array",
      "items": {
        "type": "string",
        "template": "string"
      }
    },
    "maps": {
      "type": "array",
      "description": "Map URLs for navigation",
      "template": "array",
      "items": {
        "type": "string",
        "template": "string"
      }
    },
    "generatedAt": {
      "type": "string",
      "description": "Timestamp when itinerary was generated",
      "template": "string"
    },
    "llmModel": {
      "type": "string",
      "description": "LLM model used for optimization",
      "template": "string"
    },
    "error": {
      "type": "string",
      "description": "Error message if generation failed",
      "template": "string"
    }
  }
}
